{
  "name": "members-only",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www -e js,scss,pug",
    "serverstart": "DEBUG=express-locallibrary-tutorial:* npm run devstart",
    "build:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css",
    "watch:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css --watch",
    "dev": "npm run devstart & npm run watch:css"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "luxon": "^3.0.4",
    "mongoose": "^6.6.5",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.0"
  }
}
